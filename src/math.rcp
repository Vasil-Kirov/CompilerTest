math



struct v2 {
    x: f32,
    y: f32,
}

struct v3 {
    x: f32,
    y: f32,
    z: f32,
}

struct iv3 {
    x: i32,
    y: i32,
    z: i32,
}

v2_scale :: fn(v: v2, t: f32) -> v2 {
    return v2 { v.x * t, v.y * t };
}

v2_add :: fn(vec0: v2, vec1: v2) -> v2 {
    return v2 { vec0.x + vec1.x, vec0.y + vec1.y };
}

